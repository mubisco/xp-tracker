---
default:
  suites:
    default:
      contexts:
        - XpTracker\Tests\Acceptance\DemoContext
      paths:
        - 'tests/Acceptance/features'
  extensions:
    FriendsOfBehat\SymfonyExtension:
      bootstrap: tests/bootstrap.php
      kernel:
        class: XpTracker\Kernel
        path: ~
        environment: ~
        debug: ~
